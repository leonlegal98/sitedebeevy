t":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"66","messages":"67","suppressedMessages":"68","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"69","messages":"70","suppressedMessages":"71","errorCount":0,"fatalErrorCount":0,"warningCount":16,"fixableErrorCount":0,"fixableWarningCount":0,"source":"72","usedDeprecatedRules":"35"},{"filePath":"73","messages":"74","suppressedMessages":"75","errorCount":0,"fatalErrorCount":0,"warningCount":14,"fixableErrorCount":0,"fixableWarningCount":4,"source":"76","usedDeprecatedRules":"35"},{"filePath":"77","messages":"78","suppressedMessages":"79","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"80","messages":"81","suppressedMessages":"82","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"83","usedDeprecatedRules":"35"},"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/index.js",[],[],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/App.js",[],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Home.js",["84","85","86","87","88","89","90","91","92","93","94","95","96","97","98","99","100","101","102","103","104","105","106","107","108"],[],"import '../Styles/Home.css';\nimport React, {useRef, useState, useEffect} from 'react';\n\nimport {Swiper, SwiperSlide} from 'swiper/react';\nimport {Link} from 'react-router-dom';\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport {Navigation, Pagination, Mousewheel, Keyboard} from 'swiper/modules';\nimport Menu from './Menu';\nimport Footer from './Footer';\nimport soundFile from '../sonAccueil/coeur.mp3';\nimport soundFile2 from '../sonAccueil/crayon.mp3';\n\nfunction Home() {\n    const [modalOpen,\n        setModalOpen] = useState(false);\n    const [selectedImage,\n        setSelectedImage] = useState(null);\n\n    const imageAssociations = {\n        '/images/bl4-01.png': '/images/ArticlelAlsace_1.jpg',\n        '/images/bl4-02.png': '/images/EhpadErstein_1.jpg',\n        '/images/bl4-03.png': '/images/Eurometropole_1.jpg',\n        '/images/bl4-04.png': '/images/BulletinHoenheim_1.jpg',\n        '/images/bl4-05.png': '/images/page01_1.jpg',\n        '/images/bl4-06.png': '/images/article_1.jpg',\n        // Ajoutez ici les associations pour vos images\n    };\n\n    const handleImageClick = (image) => {\n        setSelectedImage(imageAssociations[image]);\n        setModalOpen(true);\n    };\n    const closeModal = () => {\n        setSelectedImage(null);\n        setModalOpen(false);\n    };\n\n    const [audio] = useState(new Audio(soundFile));\n    const playSound = () => {\n        audio.play();\n    };\n    const [audio2] = useState(new Audio(soundFile2));\n    const playSound2 = () => {\n        audio2.play();\n    };\n    return (\n        <div className=\"homepage\">\n            <div className=\"menuleftrespon\">\n                    <Link to=\"/\">\n                        <img className='logoim1' src=\"/images/logo.png\" alt=\"Image 1\"/>\n                    </Link>\n                </div>\n\n            <div className='block1'>\n\n                <h3 className='oo'>« Écrire et se réinventer »</h3>\n\n                <p>Vous aimez écrire ? Vous ne savez pas par quoi commencer ni comment commencer ?\n                    <br></br>\n                    Rejoignez-moi dans mes ateliers d’écriture.\n\n                </p>\n                <p>Vous y trouverez un espace de créativité unique, un lieu préservé, dédié au\n                    partage et aux mots.<br></br>\n                    Guidé.es par les pistes d’écriture spécialement conçues pour l’occasion, vous\n                    serez accompagné.es\n                    <br></br>\n                    tout au long du processus. Votre créativité se déploiera hors des sentiers\n                    connus, pour votre plaisir\n                    <br></br>\n                    comme celui de vos pairs. Les échanges en groupe comme la lecture des textes\n                    vous donneront\n                    <br></br>\n                    l’opportunité d’avoir de précieux retours sur vos écrits. En toute\n                    bienveillance.</p>\n                <p>\n                    Vous hésitez? Un seul conseil : essayez ! Et si vous hésitez encore,\n                    laissez-vous porter par ces mots\n                    <br></br>\n                    de Christian Bobin «La vie est la maîtresse, j’essaie juste d’être au\n                    rendez-vous.»\n                </p>\n                <p>\n                    A très bientôt,\n                </p>\n                <div className='b1-sect1'>\n                    <img className='beevyphto' src='/images/beevyphoto.png'/>\n                    <div className='liensect1img'>\n                        <a className='beevylien' href='#'>Beevy Jalma</a>\n                        <div className='homemargeimage'>\n                            <Link to=\"https://www.linkedin.com/in/beevy-jalma-11382b1b9/\" target=\"_blank\">\n                                <img src='/images/linkedin.png'/>\n                            </Link>\n                            <Link to=\"https://www.facebook.com/latelierdebeevy\" target=\"_blank\">\n                                <img src='/images/facebook.png'/>\n                            </Link>\n                            <Link to=\"https://www.instagram.com/ecriturecreative_beevy/\" target=\"_blank\">\n                                <img src='/images/instagram.png'/>\n                            </Link>\n                            <Link to=\"https://www.youtube.com/@LatelierdeBeevy\" target=\"_blank\">\n                                <img src='/images/youtube.png'/>\n                            </Link>\n\n                        </div>\n                    </div>\n                </div>\n\n            </div>\n\n            <div className='block2'>\n                <div className=\"image-container\">\n                    <img\n                        className='imagebook'\n                        src=\"/images/imagebook.png\"\n                        onClick={playSound}\n                        style={{\n                        cursor: 'pointer'\n                    }}\n                        alt=\"Image\"/>\n                    <h1 className='textimagebl2'>Cliquez sur\n                        <br/>\n                        les images</h1>\n                    <p className='textbl2'>Des surprises vous attendent à chaque page</p>\n                </div>\n                <div className=\"text-container\">\n                    <h1 className='titrec2'>Ateliers\n                        <br></br>\n                        d’écriture</h1>\n                    <p className='parac2'>\n                        C’est avant tout une expérience à vivre. Pour vous\n                        <br></br>\n                        donner une image, je compare souvent les ateliers\n                        <br></br>\n                        d’écriture aux recettes de cuisine ou aux\n                        <br></br>\n                        entraînements sportifs. Avant d’écrire, on se\n                        <br></br>\n                        prépare.\n                    </p>\n                    <div className='lienbl2'>\n\n                        <a href='/atelier'>Découvrir</a>\n                        <a href='/texteatelier'>Textes d'ateliers</a>\n                        <a href='/tarif'>Tarifs</a>\n\n                    </div>\n                </div>\n            </div>\n            <div className='homefisrtcarousel'>\n\n                <Swiper\n                    slidesPerView={1}\n                    spaceBetween={30}\n                    loop={true}\n                    pagination={{\n                    clickable: true\n                }}\n                    navigation={true}\n                    modules={[Pagination, Navigation]}\n                    className=\"mySwiper\">\n                    <SwiperSlide>\n                        <div className='car1slide1'>\n                            <h3>« Trois heures qui sont passées comme un enchantement »</h3>\n                            <p>Sonia\n                            </p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car1slide1'>\n                            <h3>« A découvrir absolument ! »</h3>\n                            <p>Guillaume</p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car1slide1'>\n                            <h3>« Oublier l’angoisse de la page blanche ! »</h3>\n                            <p>Marie-Christine</p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car1slide1'>\n                            <h3>« Cet atelier m’a fortement comblé »</h3>\n                            <p>Félix\n                            </p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car1slide1'>\n                            <h3>« Une parenthèse enchantée »</h3>\n                            <p>Myriam</p>\n                        </div>\n                    </SwiperSlide>\n\n                </Swiper>\n            </div>\n            <div className='block3'>\n                <img\n                    onClick={playSound2}\n                    style={{\n                    cursor: 'pointer'\n                }}\n                    className='imgbl3'\n                    src='/images/groupphoto.png'/>\n                <h1 className='textbl3'>Ils me font confiance</h1>\n                <div className='homesecondcarousel'>\n                    <Swiper\n                        slidesPerView={1}\n                        spaceBetween={30}\n                        loop={true}\n                        pagination={{\n                        clickable: true\n                    }}\n                        navigation={true}\n                        modules={[Navigation]}\n                        className=\"mySwiper\">\n                        <SwiperSlide>\n                            <div className='car1slide2'>\n                                <img className='car2imgsize' src='/images/car2im1.png'/>\n                                <p>Dans le cadre du dispositif national “La classe, l'œuvre”, le Labo des\n                                    Histoires- Grand Est a organisé le cycle d’ateliers d’écriture “Sur les traces\n                                    de Théodore Deck” en partenariat avec le Musée consacré au céramiste.</p>\n                            </div>\n                        </SwiperSlide>\n                        <SwiperSlide>\n                            <div className='car1slide2'>\n                                <img className='car2imgsize' src='/images/car2im2.png'/>\n                                <p>Depuis juin 2022, j’interviens sur le territoire de La petite Pierre auprès\n                                    d’assistantes maternelles. L’occasion pour ces professionnelles de faire récit\n                                    de leurs expériences et de leurs parcours. “L'atelier de Beevy, c'est le moment\n                                    du lâcher prise” Anne Lamothe, Professionnelle du Relais Petite Enfance\n                                    d'Ingwiller</p>\n                            </div>\n                        </SwiperSlide>\n                        <SwiperSlide>\n                            <div className='car1slide2'>\n                                <img className='car2imgsize' src='/images/car2im3.png'/>\n                                <p>En collaboration avec Actemo Théâtre et Le Centre Hospitalier d’Erstein. «\n                                    Une série d'ateliers d'écriture tout en émotions, poésie et sensibilité » Céline\n                                    Modéré Adam, responsable du relais culturel\n                                </p>\n                            </div>\n                        </SwiperSlide>\n                        <SwiperSlide>\n                            <div className='car1slide2'>\n                                <img className='car2imgsize' src='/images/car2im4.png'/>\n                                <p>Depuis mars 2020, un atelier d'écriture créative par mois est proposé aux\n                                    soignantes de l'EHPAD ABRAPA.</p>\n                            </div>\n                        </SwiperSlide>\n                        <SwiperSlide>\n                            <div className='car1slide2'>\n                                <img className='car2imgsize' src='/images/car2im5.png'/>\n                                <p>« Cet atelier à permis aux participants de renouer avec les autres de\n                                    retrouver confiance » Sabrina Helbling, co-administratice\n                                </p>\n                            </div>\n                        </SwiperSlide>\n                        <SwiperSlide>\n                            <div className='car1slide2'>\n                                <img className='car2imgsize' src='/images/car2im6.png'/>\n                                <p>Des ateliers d'écriture proposés à un public de migrants du centre de\n                                    Ferrette. L’occasion d’aborder les thèmes de l'égalité, de la liberté, de la\n                                    justice avec des hommes et des femmes venus des quatre coins du monde.\n                                </p>\n                            </div>\n                        </SwiperSlide>\n\n                    </Swiper>\n                    <a href='confiance'>Voir plus</a>\n                </div>\n            </div>\n\n            <div className='block4'>\n                {Object\n                    .keys(imageAssociations)\n                    .map((image, index) => (<img\n                        key={index}\n                        src={image}\n                        alt={`Image ${index}`}\n                        onClick={() => handleImageClick(image)}/>))}\n            </div>\n            {modalOpen && (\n                <div className=\"modal-overlay\" onClick={closeModal}>\n                    <div className=\"modal\">\n                        <div className=\"modal-header\">\n                            <button className=\"close-button\" onClick={closeModal}>X</button>\n                        </div>\n                        {selectedImage && <img src={selectedImage} alt=\"Image en grand\"/>}\n                    </div>\n                </div>\n            )}\n            <div className='block5'>\n                <h1 className='titrebl5'>Suivez-moi sur les réseaux</h1>\n                <p className='textbl5'>Pour être au courant de toute mon actu et des ateliers d’écriture à venir</p>\n                <div className='imgbl5'>\n                    <img src=\"/images/imgbl5-01.png\"/>\n                    <img src=\"/images/imgbl5-02.png\"/>\n                    <img src=\"/images/imgbl5-03.png\"/>\n                </div>\n                <div className='reseausocaiuxhome'>\n                    <a className='cparici' href='https://www.instagram.com/ecriturecreative_beevy/'>Instagram &gt;</a>\n                    <a className='cparici' href='https://www.facebook.com/latelierdebeevy'>Facebook &gt;</a>\n                </div>\n\n            </div>\n            <Footer/>\n        </div>\n    );\n}\n\nexport default Home;\n","/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Menu.js",["109","110","111","112","113","114"],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Bio.js",["115","116","117","118","119","120","121","122","123","124","125","126","127","128","129"],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Atelier.js",["130","131","132","133","134","135","136","137","138","139","140","141","142","143","144"],[],"import '../Styles/Atelier.css';\n\nimport React, {useState} from 'react';\nimport Bio from './Bio.js';\nimport Home from './Home.js';\nimport {Swiper, SwiperSlide} from 'swiper/react';\nimport 'swiper/css';\nimport 'swiper/css/navigation';\nimport 'swiper/css/pagination';\nimport {Navigation, Pagination, Mousewheel, Keyboard} from 'swiper/modules';\nimport Footer from './Footer';\nimport soundFile1 from '../sonAtelier/mainquiecrit.mp3';\nimport soundFile2 from '../sonAtelier/sonrobin.mp3';\nimport soundFile3 from '../sonAtelier/graindecafe.mp3';\nimport soundFile4 from '../sonAtelier/citationcioran.mp3';\n\nfunction Atelier() {\n\n    const [audio] = useState(new Audio(soundFile1));\n    const playSound1 = () => {\n        audio.play();\n    };\n    const [audio2] = useState(new Audio(soundFile2));\n    const playSound2 = () => {\n        audio2.play();\n    };\n    const [audio3] = useState(new Audio(soundFile3));\n    const playSound3 = () => {\n        audio3.play();\n    };\n    const [audio4] = useState(new Audio(soundFile4));\n    const playSound4 = () => {\n        audio4.play();\n    };\n\n    const [contentStates,\n        setContentStates] = useState({});\n\n    const toggleContent = (id) => {\n        setContentStates((prevState) => ({\n            ...prevState,\n            [id]: !prevState[id]\n        }));\n    };\n    const textData = [\n        {\n            id: 'text1',\n            title: '« Je n’ai jamais participé à un atelier d’écriture, est-ce accessible aux débuta' +\n                    'nts ? »',\n            content: 'Oui, tous les ateliers sont accessibles aux débutant.es.'\n        }, {\n            id: 'text2',\n            title: '« Je fais des fautes d’orthographe, est-ce grave ? »',\n            content: 'Il serait dommage de vous priver d’un atelier d’écriture pour des raisons orthog' +\n                    'raphiques ou grammaticales. À ce sujet, Bukowski disait :«  Je n’ai pas prêté b' +\n                    'eaucoup d’attention à la grammaire, et quand j’écris c’est surtout par amour du ' +\n                    'mot, de la tonalité, comme un peintre jetant ses couleurs sur la toile. (...) Un' +\n                    'e écriture qui vient du sang plutôt que de l’intellect. »'\n        }, {\n            id: 'text3',\n            title: '« Pour participer à une séance, faut-il s’inscrire à tous les ateliers ? »',\n            content: 'Non, vous pouvez vous inscrire à une seule séance. Les ateliers proposés sont in' +\n                    'dépendants les uns des autres.'\n        }, {\n            id: 'text4',\n            title: '« Y a-t-il quelque chose à savoir avant de participer? »',\n            content: 'Aucune connaissance n’est requise pour participer à un atelier.'\n        }, {\n            id: 'text5',\n            title: '« Suis-je obligé.e de lire mon texte en groupe? »',\n            content: 'Il n’y a aucune obligation en la matière, chacun.e est libre de lire ou non son ' +\n                    'texte en atelier.'\n        }, {\n            id: 'text6',\n            title: '« Que dois-je emmener ? »',\n            content: 'De quoi écrire. En cas d’oubli, je mets à disposition feuilles blanches et stylo' +\n                    's.'\n        }, {\n            id: 'text7',\n            title: '« Qu’est-ce que ça va m’apporter ? »',\n            content: 'A cette question, vous seul.e pouvez répondre.'\n        }, {\n            id: 'text8',\n            title: '« Est-ce que je vais apprendre quelque chose ? »',\n            content: 'Même réponse que précédemment. Et j’ajouterai : je ne sais pas si vous allez « a' +\n                    'pprendre quelque chose », mais je suis convaincue que vous aurez vécu un moment ' +\n                    'unique.'\n        }, {\n            id: 'text9',\n            title: '« Ça se passe où et quand ? »',\n            content: 'Les ateliers d’écriture créative destinés aux particuliers se déroulent le samed' +\n                    'i, une fois par mois, dans un lieu historique (inscrit au patrimoine mondial de ' +\n                    'l’Unesco), situé au cœur de la Grande-île de Strasbourg. La salle est accessible' +\n                    ' aux personnes à mobilité réduite. Pour les établissements faisant appel à mes s' +\n                    'ervices, je m’en remets à la structure pour mettre à disposition une salle adapt' +\n                    'ée à la bonne tenue de la séance.'\n        }, {\n            id: 'text10',\n            title: '« Combien ça coûte ? »',\n            content: 'Voir \"Tarifs\"'\n        },\n\n    \n\n    ];\n\n    return (\n\n        <div className=\"atelierbody\">\n            <div className='ateliersect1'>\n                <div className='ateliersect1b1'>\n                    <h1>Un atelier d’écriture <br></br> c’est quoi ?</h1>\n                    <p>Un atelier d’écriture est un lieu regroupant des personnes qui écrivent. Pour\n                        reprendre les mots d’Odile Neumayer, « en atelier, il n’y a pas de hors-sujet »\n                        car « c’est vous le sujet. » Ouvert.es à toustes, sans aucune restriction , mes\n                        ateliers se déroulent en Alsace, en « présentiel » ou en « visio ». Je peux\n                        également me déplacer dans toute la France.\n                    </p>\n                    <div className='liensect1'>\n                        <a href='/texteatelier'>Textes d’ateliers</a>\n                        <a href='podcast'>Podcasts</a>\n                        <a href='tarif'>Tarifs</a>\n                    </div>\n                    <div className='bioateletierbaudelaire'>\n                        <h4>« Parce que la forme est contraignante,\n                            <br></br>\n                            l'idée jaillit plus intense »</h4>\n                        <p>\n                            Charles Baudelaire</p>\n                    </div>\n                </div>\n\n                <img\n                    src=\"/images/atelier-01.png\"\n                    alt=\"Image 1\"\n                    onClick={playSound1}\n                    style={{\n                    cursor: 'pointer'\n                }}/>\n            </div>\n\n            <div className='ateliersect2'>\n\n                <img\n                    src=\"/images/atelier-02.png\"\n                    alt=\"Image 2\"\n                    onClick={playSound2}\n                    style={{\n                    cursor: 'pointer'\n                }}/>\n                <div className='ateliersect2b2'>\n                    <h1>Comment se passe\n                        <br></br>\n                        un atelier ?</h1>\n                    <p>Il m’est difficile de répondre à cette question. Que dire ? Quoi décrire ?\n                        Chaque atelier est unique. C’est avant tout une expérience à vivre. Pour vous\n                        donner une image, je compare souvent les ateliers d’écriture aux recettes de\n                        cuisine ou aux entraînements sportifs. Avant d’écrire, on se prépare.\n\n                    </p>\n                </div>\n            </div>\n            <div className='ateliersect3'>\n                <div className='ateliersect3b1'>\n\n                    <p>Comme en cuisine, on réunit les ingrédients (en atelier, les mots), puis on\n                        les pèse, on les goûte, on les assaisonne, on les associe. Chaque plat est\n                        unique, chaque écrit l’est aussi. La saveur diffère selon la sensibilité de\n                        l’auteur.e, son humeur, son implication, les procédés utilisés.\n                    </p>\n                    <p>\n                        Tout au long du processus, je suis présente et je vous guide, avec des pistes\n                        d’écritures adaptées. Vient ensuite le temps de la dégustation-lecture, moment\n                        de partage entre pairs.\n\n                    </p>\n                    <p>« Parlez-moi d’amour », « Sur les chemins », « Un air de vacances », «\n                        Portraits de famille » sont quelques exemples de thématiques proposées aux\n                        particuliers.\n                    </p>\n                    <p>\n                        J’interviens également auprès de<span className='textesurligneratelier'>\n                            professionnelles en établissement et en entreprise.</span>\n                        Là, les ateliers mêlent écriture et analyse de pratiques.\n                    </p>\n                </div>\n                <img\n                    src=\"/images/atelier-03.png\"\n                    alt=\"Image 3\"\n                    onClick={playSound3}\n                    style={{\n                    cursor: 'pointer'\n                }}/>\n            </div>\n            <div className='ateliersect4'>\n                <img\n                    src=\"/images/atelier.png\"\n                    alt=\"Image 3\"\n                    onClick={playSound4}\n                    style={{\n                    cursor: 'pointer'\n                }}/>\n\n                <div className='ateliersect4b1'>\n                    <h1>Ma spécialité</h1>\n                    <p>Mes années d’éducatrice spécialisée, doublées de mon cheminement personnel et\n                        thérapeutique, m’ont permis de développer des qualités d’écoute, de\n                        bienveillance, de respect et d’accueil de l’autre. L’engagement sincère qui\n                        était le mien dans l’exercice de mes fonctions précédentes se retrouve dans les\n                        ateliers d’écriture que j’anime, mis au service des écrivants que je rencontre.</p>\n                    <p>Grâce à mon expérience et à ma formation, je suis outillée pour animer des\n                        ateliers auprès de divers professionnels, notamment du secteur sanitaire,\n                        médico-social et éducatif. Dans le cadre de \" plan de formation \", des sessions\n                        d’ateliers d’écriture peuvent être organisées avec des thèmes définis au\n                        préalable par les équipes comme par exemple : \" la bientraitance \", \" comment\n                        faire récit de la souffrance \"ou encore \" mettre en mots les missions des\n                        professionnels, de l'exécution à la pensée \".</p>\n                </div>\n            </div>\n            <div className='ateliersection5'>\n                <h1 className='textateliersect5'>Ils me font confiance</h1>\n                <Swiper\n                    slidesPerView={1}\n                    spaceBetween={30}\n                    loop={true}\n                    pagination={{\n                    clickable: true\n                }}\n                    navigation={true}\n                    modules={[Navigation]}\n                    className=\"mySwiper\">\n                    <SwiperSlide>\n                        <div className='car3slide2'>\n                            <img className='car2imgsize' src='/images/car2im1.png'/>\n                            <p>Dans le cadre du dispositif national “La classe, l'œuvre”, le Labo des\n                                <br></br>\n                                Histoires- Grand Est a organisé le cycle d’ateliers d’écriture “Sur les\n                                <br></br>\n                                traces de Théodore Deck” en partenariat avec le Musée consacré\n                                <br></br>\n                                au céramiste et le collège Mathias Grünewald de Guebwiller.</p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car3slide2'>\n                            <img className='car2imgsize' src='/images/car2im2.png'/>\n                            <p>Depuis juin 2022, j’interviens sur le territoire de La petite Pierre\n                                <br></br>\n                                auprès d’assistantes maternelles. L’occasion pour ces\n                                <br></br>\n                                professionnelles de faire récit de leurs expériences et de leurs\n                                <br></br>\n                                parcours. “L'atelier de Beevy, c'est le moment du lâcher prise”\n                                <br></br>\n                                Anne Lamothe, Professionnelle du Relais Petite Enfance d'Ingwiller</p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car3slide2'>\n                            <img className='car2imgsize' src='/images/car2im3.png'/>\n                            <p>En collaboration avec Actemo Théâtre et Le Centre Hospitalier\n                                <br></br>\n                                d’Erstein. « Une série d'ateliers d'écriture tout en émotions, poésie\n                                <br></br>\n                                et sensibilité » Céline Modéré Adam, responsable du relais culturel\n                            </p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car3slide2'>\n                            <img className='car2imgsize' src='/images/car2im4.png'/>\n                            <p>Depuis mars 2020, un atelier d'écriture créative par mois est\n                                <br></br>\n                                proposé aux soignantes de l'EHPAD ABRAPA.</p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car3slide2'>\n                            <img className='car2imgsize' src='/images/car2im5.png'/>\n                            <p>« Cet atelier à permis aux participants de renouer avec les autres,<br></br>\n                                de retrouver confiance » Sabrina Helbling, co-administratice\n                            </p>\n                        </div>\n                    </SwiperSlide>\n                    <SwiperSlide>\n                        <div className='car3slide2'>\n                            <img className='car2imgsize' src='/images/car2im6.png'/>\n                            <p>Des ateliers d'écriture proposés à un public de migrants du centre\n                                <br></br>\n                                de Ferrette. L’occasion d’aborder les thèmes de l'égalité, de la\n                                <br></br>liberté, de la justice avec des hommes et des\n                                <br></br>\n                                femmes venus des quatre coins du monde.\n                            </p>\n                        </div>\n                    </SwiperSlide>\n\n                </Swiper>\n                <a href='confiance'>Voir plus</a>\n            </div>\n\n            <div className='faqatelier'>\n                {textData.map((item) => (\n                    <div className='ligneatelierfoo' key={item.id}>\n                        <h4 className=\"textfooteratelier\">\n                            {item.title}\n                            <button\n                                className={`toggle-button-atelier ${contentStates[item.id]\n                                ? 'minus'\n                                : ''}`}\n                                onClick={() => toggleContent(item.id)}>\n                                {contentStates[item.id]\n                                    ? (\n                                        <img className='moinsfaq' src=\"/images/-.png\" alt=\"Minus\"/> // Image pour fermer le contenu\n                                    )\n                                    : (\n                                        <img className='plusfaq' src=\"/images/+.png\" alt=\"Plus\"/> // Image pour ouvrir le contenu\n                                    )}\n                            </button>\n                        </h4>\n                        <div\n                            className={`toggle-content-atelier ${contentStates[item.id]\n                            ? 'show'\n                            : ''}`}>\n                            <p>{item.content}</p>\n                        </div>\n                    </div>\n                ))}\n            </div>\n\n            <Footer/>\n\n        </div>\n\n    );\n}\n\nexport default Atelier;\n","/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Presse.js",["145","146","147","148","149","150","151","152","153","154","155","156","157","158","159"],[],"import '../Styles/Presse.css';\n\nimport Bio from './Bio.js';\nimport Home from './Home.js';\nimport Footer from './Footer';\nimport React, {useRef, useState, useEffect} from 'react';\n\nfunction Presse() {\n    const [modalOpen,\n        setModalOpen] = useState(false);\n    const [selectedImage,\n        setSelectedImage] = useState(null);\n\n    const imageAssociations = {\n        '/images/1.png': '/images/ArticlelAlsace_1.jpg',\n        '/images/2.png': '/images/EhpadErstein_1.jpg',\n        '/images/3.png': '/images/Eurometropole_1.jpg',\n        '/images/4.png': '/images/BulletinHoenheim_1.jpg',\n        '/images/5.png': '/images/page01_1.jpg',\n        '/images/6.png': '/images/article_1.jpg',\n        // Ajoutez ici les associations pour vos images\n    };\n\n    const handleImageClick = (image) => {\n        setSelectedImage(imageAssociations[image]);\n        setModalOpen(true);\n    };\n    const closeModal = () => {\n        setSelectedImage(null);\n        setModalOpen(false);\n    };\n    return (\n\n        <div className=\"pressebody\">\n\n            <div className='titrepresse'>\n                <h1>Presse écrite</h1>\n            </div>\n            <div className='listeimagepress'>\n                {Object\n                    .keys(imageAssociations)\n                    .map((image, index) => (<img\n                        key={index}\n                        src={image}\n                        alt={`Image ${index}`}\n                        onClick={() => handleImageClick(image)}/>))}\n            </div>\n            {modalOpen && (\n                <div className=\"modal-overlay\" onClick={closeModal}>\n                    <div className=\"modal\">\n                        <div className=\"modal-header\">\n                            <button className=\"close-button\" onClick={closeModal}>X</button>\n                        </div>\n                        {selectedImage && <img src={selectedImage} alt=\"Image en grand\"/>}\n                    </div>\n                </div>\n            )}\n\n            <div className='titrepressedeux'>\n                <h1>Télévision</h1>\n            </div>\n            <div className='pressbl1'>\n                <div>\n                    \n                <a href='https://www.youtube.com/watch?v=ehSTKBNhjj0' target=\"_blank\">\n                <img src='/images/pressbl1.png'/>\n                </a>\n                </div>\n                \n                \n                <div className='pressbl1bl2'>\n                    <h3>Les ateliers d'écriture en confinement\n                    </h3>\n                    <p>\n                        <span>16/10/2020</span>\n                        <br></br>\n                        -\n                        <br></br>\n                        <a href='https://www.youtube.com/watch?v=ehSTKBNhjj0' target=\"_blank\">Un reportage de Sylvie Malal</a> sur les ateliers d'écriture en EHPAD diffusé dans\n                        le 19/20 de France 3 Alsace\n                    </p>\n                </div>\n            </div>\n            <div className='pressbl2'>\n                <div>\n                <a href='https://www.youtube.com/watch?v=5XiJvngx5Ew' target=\"_blank\">\n                <img src='/images/press-02.png'/>\n                </a>\n                </div>\n               \n                <div className='pressbl2bl2'>\n                    <h3>Les ateliers d'écriture en confinement\n                    </h3>\n                    <p>\n                        <span>13/12/2021</span>\n                        <br></br>\n                        -\n                        <br></br>\n                        <a href='https://www.youtube.com/watch?v=5XiJvngx5Ew' target=\"_blank\">Un reportage de Jean-Marc Schaefer</a> pour TV Erstein sur les ateliers d’écriture <br></br>au Centre Hospitalier \n                    </p>\n                </div>\n            </div>\n\n            <div className='titrepressetrois'>\n                <h1>Radio</h1>\n            </div>\n\n            <div className='pressbl3'>\n                <div>\n                    <a href='https://www.youtube.com/watch?v=PKGzcsYY7Ys' target=\"_blank\">\n                <img src='/images/press-03.png'/>\n                </a>\n                </div>\n                \n                <div className='pressbl3bl2'>\n                    <h3>Les ateliers d'écriture en confinement\n                    </h3>\n                    <p>\n                        <span>01/03/2021</span>\n                        <br></br>\n                        -\n                        <br></br>\n                        <a href='https://www.youtube.com/watch?v=PKGzcsYY7Ys' target=\"_blank\">Une chronique d'Olivier Vogel</a>, journal de  7 heures, sur France Bleu Alsace\n                    </p>\n                </div>\n            </div>\n\n            <Footer/>\n        </div>\n\n    );\n}\n\nexport default Presse;\n","/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Contact.js",["160"],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Tarif.js",["161","162"],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/TexteAtelier.js",[],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Creative.js",[],[],"/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Confiance.js",["163","164","165","166","167","168","169","170","171","172","173","174","175","176","177","178"],[],"import React from 'react';\n\nimport '../Styles/Confiance.css';\nimport Footer from './Footer';\n\nfunction Confiance() {\n    return (\n        <div className='confiancehome'>\n            <div className='titreconfiance'>\n                <h1>Ils me font confiance</h1>\n\n            </div>\n            <div className='containerconfiance'>\n                <div className='confiancearticle'>\n                    <img src='/images/conf1.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Le Labo des Histoires Grand Est\n                        </h3>\n                        <p>\n                            Dans le cadre du dispositif national “La classe, l'œuvre”, le Labo des\n                            Histoires- Grand Est a organisé le cycle d’ateliers d’écriture “Sur les traces\n                            de Théodore Deck” en partenariat avec le Musée consacré au céramiste et le\n                            collège Mathias Grünewald de Guebwiller.\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf2.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Le Lycée Polyvalent Louis Marchal (Molsheim)\n                        </h3>\n                        <p>\n                            Deux ateliers d’écriture sur le thème des frontières, auprès d’une classe de\n                            seconde du Lycée polyvalent Louis Marchal de Molsheim. Retrouvez les écrits des\n                            lycéens sur la page \"Textes d'ateliers\".\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf3.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Le Lycée ORT\n                        </h3>\n                        <p>\n                            Deux ateliers d'écriture créative auprès de lycéens de classe de seconde\n                            générale, dans le cadre du Printemps des Poètes 2023.\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf4.png'/>\n                    <div className='confiancebl2'>\n                        <h3>La Bibliothèque d’Alsace\n                        </h3>\n                        <p>\n                            Dans le cadre de la saison culturelle de la Collectivité européenne d’Alsace,\n                            des ateliers d'écriture ont été proposés à un public de migrants du centre de\n                            Ferrette (Haut-Rhin). L’occasion d’aborder les thèmes de l'égalité, de la\n                            liberté, de la justice avec des hommes et des femmes venus des quatre coins du\n                            monde.\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf5.png'/>\n                    <div className='confiancebl2'>\n                        <h3>La médiathèque d'Holtzheim et le Relais Petite Enfance de la commune\n                        </h3>\n                        <p>\n                            Le Relais Petite Enfance et la médiathèque Marceau ont proposé un atelier\n                            d'écriture créative aux assistantes maternelles d'Holtzheim afin de mettre en\n                            mots leur parcours.\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf6.png'/>\n                    <div className='confiancebl2'>\n                        <h3>L'ERPD, Établissement Régional du Premier Degré\n                        </h3>\n                        <p>\n                            D'octobre à décembre 2022, dans le cadre du projet des cigognes en Afrique, des\n                            ateliers d’écriture ont été proposés aux élèves de l'ERPD ainsi qu’aux enfants\n                            du Centre Jacoutôt-Braille (public déficient auditif). Les productions ont donné\n                            lieu à des textes de slam mis en musique par les élèves.\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf7.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Le Relais Petite Enfance de la Communauté de Communes Hanau-La Petite Pierre\n                        </h3>\n                        <p>\n                            Depuis juin 2022, j’interviens sur le territoire de sur le territoire de\n                            Hanau-La Petite Pierre auprès d’assistantes maternelles. L’occasion pour ces\n                            professionnelles de faire récit de leurs expériences et de leurs parcours.<br></br>\n                            \"L'atelier de Beevy, c'est le moment du lâcher prise. Créatifs, intuitifs, ils\n                            permettent de mettre son vécu en mots, de jouer avec ses mots et les rendre\n                            beaux, vivants, touchants. L 'atelier de Beevy, c'est mettre ses maux brut sur\n                            le papier, de découvrir avec émotion la douce poésie dans le quotidien des\n                            assistantes Maternelles et de mettre en valeur ce métier !\" Anne Lamothe -\n                            Référente Parentalité, Relais Petite Enfance, Maison de la Famille d'Ingwiller.\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf8.png'/>\n                    <div className='confiancebl2'>\n                        <h3>L'Ehpad ABRAPA à Hoenheim\n                        </h3>\n                        <p>\n                            Depuis mars 2020, un atelier d'écriture créative par mois est proposé aux\n                            soignantes de l'EHPAD ABRAPA. \"Au sein d’un EHPAD, il y a des hommes et des\n                            femmes qui se rencontrent, des histoires qui se croisent, des réalités qui se\n                            confrontent. Chacun fait face à des situations qui les bousculent : la mort, la\n                            solitude, la maladie, la dépendance, l’isolement, l’intimité, l’amour,\n                            l’amitié... C’est un lieu où les affects nous traversent sans cesse. Dans les\n                            ateliers de Beevy, nous, professionnels et sujets singuliers, partageons nos\n                            ressentis qui se canalisent dans un cadre et par l’écrit. Accompagnés et guidés,\n                            chacun peut exprimer ce qu’il fallait panser, entendre ce que l’autre à\n                            apporter, accorder ses crayons, conjuguer ses actions, trouver l’inspiration,\n                            l’expirer et transformer l’essai. Chacun vient comme il est, il suffit de se\n                            laisser porter et grâce aux thèmes abordés et aux consignes bien pensées, une\n                            trace est laissée en toute simplicité pour nous soulager et dévoiler la poésie\n                            qui est au cœur de nos métiers.\"Aurélia Preslier, psychologue clinicienne\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf9.png'/>\n                    <div className='confiancebl2'>\n                        <h3>L'ESEIS\n                        </h3>\n                        <p>\n                            Depuis septembre 2021, j'interviens en tant qu'artiste et formatrice\n                            occasionnelle au sein de l’École Supérieure Européenne d'Intervention Sociale\n                            auprès d’un public d’étudiants (éducateurs spécialisés, éducateurs techniques et\n                            assistants de services sociaux).\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf10.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Les ateliers des Malteries\n                        </h3>\n                        <p>\n                            Au printemps/été 2021, j'ai animé des ateliers d'écriture au Théâtre des mots\n                            des Ateliers des Malteries à Schiltigheim, en collaboration avec Amélie Enon,\n                            metteure en scène. \"Les interventions de Beevy au sein de l’atelier des\n                            Malteries - théâtre des mots, a permis de prolonger nos expérimentations\n                            théâtrales imaginées à partir d’entretiens enregistrés et retranscrits. Il\n                            s’agissait d’inviter les participants et futurs acteurs à imaginer une écriture\n                            théâtrale à partir de ce matériau narratif. Quelle distance pouvons-nous prendre\n                            avec l’original ? Comment inventer l’écriture de ce qui a été dit et collecté ?\n                            Ces séances ont aussi été le moyen de découvrir le théâtre par un autre versant\n                            : non plus uniquement celui du plateau mais également celui de l’écriture, pour\n                            rendre les deux actions perméables l’une à l’autre. Je remercie Beevy de nous\n                            avoir accompagné dans cette recherche.\" Amélie Enon, metteure en scène \"Beevy\n                            Jalma a assuré l’animation de notre atelier « Le théâtre des mots » de mai à\n                            juillet 2021. Sa contribution a permis de structurer et consolider très\n                            utilement les travaux poursuivis dans cet atelier en vue d’une représentation, «\n                            Le Grand Récit », intervenue au cours du festival MUZ 2022, organisé par notre\n                            association en mai 2022.\" L'équipe associative des Ateliers des Malteries\n\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf11.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Actémo Théâtre\n                        </h3>\n                        <p>\n                            Dans le cadre du spectacle \"Vous êtes ici, Dans ma poche d'exilée, un flocon de\n                            neige\" la compagnie de théâtre m’a sollicitée pour animer des ateliers\n                            d'écriture en lien avec le sujet du spectacle : l'évacuation des Alsaciens vers\n                            la Dordogne durant la seconde guerre mondiale. \"Les interventions de Beevy au\n                            sein de l’EHPAD d'Erstein ont été l'occasion d'un beau prolongement des\n                            représentations d'un de nos spectacles qui traitait de l'évacuation des\n                            Alsaciens vers la Dordogne en 1939. Collaborer avec Beevy a été un véritable\n                            plaisir. Avec douceur et professionnalisme, elle a élaboré et mené des\n                            interventions parfaitement adaptées aux résident.es de l'EHPAD. Elle a su les\n                            mettre en confiance et faire émerger des textes riches et de grande qualité.\n                            Puis, visiteurs et résident.es ont pu visiter une exposition scénographique\n                            installée le hall de l'Hôpital d'Erstein et écouter les textes enregistrés par\n                            des comédiens dialectophones. Des moments d'ateliers sont disponibles sous la\n                            forme d'un Podcast \" Delphine Crubézy, Directrice artistique de la Compagnie\n                            Actémo Théâtre\n\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf12.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Le Centre Hospitalier d'Erstein -Ehpad \"Les Colibris\"\n                        </h3>\n                        <p>\n                            Centre Hospitalier d'Erstein. \"Beevy Jalma est intervenue au sein de notre\n                            établissement de novembre 2021 à mars 2022 auprès d'un groupe de six résidents\n                            de l'Ehpad et de l'Unité de Soin Longue Durée, dans le cadre d'un projet initié\n                            par le relais culturel de la ville d'Erstein. Ainsi, durant six séances, Beevy a\n                            proposé des ateliers d'écriture sur le thème de l'évacuation des alsaciens en\n                            Dordogne durant la seconde guerre mondiale. Par le biais d'objets anciens, les\n                            résidents, guidés avec une extrême bienveillance par Beevy, ont mobilisé leurs\n                            souvenirs, les ont retranscrits à l' oral puis enfin, à la manière de certains\n                            auteurs, comme Paul Eluard, ont écrit ce que les objets leur évoquaient ...Pour\n                            finir, une captation audio a permis, en lien avec la compagnie Actémo Théâtre de\n                            diffuser ces textes lors d'une exposition au Centre Hospitalier d'Erstein. Outre\n                            le fait de mobiliser des souvenirs anciens,ces ateliers ont permis à nos\n                            résidents de se sentir \"revivre\". L'écoute bienveillante et attentive de Beevy,\n                            sa gentillesse et son attention toute particulière ainsi que son savoir faire,\n                            ont permis aux résidents de mobiliser des capacités souvent en sommeil dans les\n                            Ehpad, à savoir, la mémoire mais aussi la concentration, l'intérêt, le désir et\n                            la sensation d'exister pour entreprendre quelque chose: Écrire et faire vivre\n                            des récits dans une narration partagée, sous forme de lecture ou d'exposition à\n                            destination du grand public. Ces séances furent une véritable bouffée d'oxygène\n                            pour nos résidents, brillamment menée par la douceur et le professionnalisme de\n                            Beevy. A refaire sans hésiter...\" Laurence Ottogalli, Cadre supérieure du pôle\n                            personnes âgées Centre Hospitalier d'Erstein\n\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf13.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Le Relais Culturel de la Ville d'Erstein\n                        </h3>\n                        <p>\n                            La ville d'Erstein a soutenu le projet de création d’Actémo Théâtre et les\n                            ateliers d'écriture créative en Ehpad. \"Le contact chaleureux de Beevy Jalma et\n                            son travail auprès d'un groupe de résidents d'Ehpad et d'USLD leur a permis de\n                            raviver des souvenirs de façon artistique, à travers une série d'ateliers\n                            d'écriture tout en émotions, poésie et sensibilité. Des bulles d'imaginaire dont\n                            ils ont, chacun, salué le bénéfice immédiat et les répercussions positives dans\n                            leur quotidien, Ceci sans compter le retour de leurs familles et de l'équipe\n                            soignante, qui ont (re)découvert à travers leurs écrits leurs capacités\n                            créatives insoupçonnées! Ce projet, mené en lien avec la compagnie Actémo\n                            Théâtre, a également pris tout son sens à travers la création de capsules audio\n                            mettant en valeur ce travail d'écriture et permettant de le partager plus\n                            largement. Merci pour ce savoir-faire généreux et exigeant ! \"Céline\n                            Modéré-Adam, responsable du Relais culturel d'Erstein, initiateur et partenaire\n                            de cette action culturelle\n\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf14.png'/>\n                    <div className='confiancebl2'>\n                        <h3>Les Petites Cantines\n                        </h3>\n                        <p>Dans le cadre des \"ateliers déconfinés\", en novembre 2021, un atelier\n                            d'écriture créative « se retrouver » a été proposé aux adhérents du restaurant\n                            collaboratif. \"Beevy a animé un atelier d'écriture dans les locaux des Petites\n                            Cantines de Strasbourg. Elle a su rapidement mettre le groupe en confiance avec\n                            douceur et bienveillance, nous accompagnant progressivement et avec\n                            professionnalisme. Ses commentaires et son expérience ont été d'une grande aide\n                            dans le processus d'écriture. Cet atelier à permis aux participants de renouer\n                            avec soi et les autres, de retrouver la confiance en soi à travers une\n                            créativité parfois insoupçonnée, de s'amuser et de se faire plaisir.\" Sabrina\n                            Helbling, co-administratice des Petites Cantines\n\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf15.png'/>\n                    <div className='confiancebl2'>\n                        <h3>ABL Europe à Illkirch\n                        </h3>\n                        <p>\n                            En septembre 2021, des ateliers d'écriture ont été proposés aux salariés d'ABL\n                            EUROPE (Illkirch-Graffenstaden) dans le cadre de la \"semaine du bien-être\".\n                        </p>\n                    </div>\n\n                </div>\n                <div className='confiancearticle'>\n                    <img src='/images/conf16.png'/>\n                    <div className='confiancebl2'>\n                        <h3>L'Association Rue Méditerranée\n                        </h3>\n                        <p>\n                            En juillet 2021, j'ai animé un atelier d'écriture à la galerie Art Course à\n                            partir de l'exposition \"Méditerranée, les couleurs de la mer\" réunissant les\n                            œuvres de quinze artistes.\n                        </p>\n                    </div>\n\n                </div>\n\n            </div>\n            <Footer/>\n\n        </div>\n    );\n}\n\nexport default Confiance;\n","/Users/leon.le-gal/Desktop/Site de Beevy/beevy/src/Components/Footer.js",["179","180","181","182","183","184","185","186","187","188","189","190","191","192"],[],"import '../Styles/Footer.css';\nimport '../Styles/Menu.css';\nimport React from 'react';\nimport {BrowserRouter as Router, Link, Routes, Route} from 'react-router-dom';\n\n\n\n\nfunction Footer() {\n    return (\n\n        <div className=\"\">\n             <div className='footer'>\n                <div className='footerb1'>\n                    <img src='/images/white.png'/>\n                </div>\n                <div className='footerb2'>\n                    <h3>Menu</h3>\n                    <div className='footerb2b1'>\n                        <a href='bio'>Bio</a>\n                        <a href='atelier'>Ateliers</a>\n                        <a href='tarif'>Tarifs</a>\n                        <a href='presse'>Presse</a>\n                        <a href='contact'>Contact</a>\n                    </div>\n\n                    <div className='footerb2b2'>\n                        <a>Mentions légales</a>\n                        <a>CGV</a>\n                    </div>\n                </div>\n                <div className='footerb3'>\n                    <div className='footerb3b1'>\n                        <h3>Contact</h3>\n                        <a className='emailfooter' href='/contact'>Email</a>\n                            <div className='socialfooter'>\n                            <Link to=\"https://www.linkedin.com/in/beevy-jalma-11382b1b9/\" target=\"_blank\">\n                            <img src='/images/linkedin.png'/>\n                            </Link>\n                            <Link to=\"https://www.facebook.com/latelierdebeevy\" target=\"_blank\">\n                            <img sr