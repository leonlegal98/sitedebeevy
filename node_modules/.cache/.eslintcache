        date: '13:56',\n            texte: 'Dans la valise'\n        }, {\n            key: 'episode2',\n            title: 'Episode 2',\n            url: '/podcasts/Episode2.mp3',\n            date: '10:10',\n            texte: 'Dans le chapeau'\n        }, {\n            key: 'episode3',\n            title: 'Episode 3',\n            url: '/podcasts/Episode3.mp3',\n            date: '10:46',\n            texte: 'Dans la thermos'\n        }, {\n            key: 'episode4',\n            title: 'Episode 4',\n            url: '/podcasts/Episode4.mp3',\n            date: '12:56',\n            texte: 'Dans la radio'\n        }, {\n            key: 'episode5',\n            title: 'Episode 5',\n            url: '/podcasts/Episode5.mp3',\n            date: '14:56',\n            texte: 'Dans la boîte à biscuits'\n        },\n        // Ajoutez d'autres podcasts ici\n    ];\n\n    const togglePlayPause = (url) => {\n        const audio = audioRef.current;\n        if (currentlyPlaying === url) {\n            if (isPaused) {\n                audio.play();\n                setIsPaused(false);\n            } else {\n                audio.pause();\n                setIsPaused(true);\n            }\n        } else {\n            audio.src = url;\n            audio.play();\n            setCurrentlyPlaying(url);\n            setIsPaused(false);\n        }\n    };\n\n    const downloadPodcast = (url) => {\n        const link = document.createElement('a');\n        link.href = url;\n        link.download = 'podcast.mp3';\n        link.click();\n    };\n\n    return (\n        <div>\n            <div className='titrepodcast'>\n                <h1>Podcasts</h1>\n            </div>\n\n            <div className='podcastbl1'>\n                <div>\n                    <img src='/images/imagepodcast.png'/>\n                </div>\n\n                <div className='podcastbl1bl2'>\n                    <h3 className='poddansma'>Dans ma poche,\n                        ma mémoire\n                    </h3>\n                    <p>\n                        <span>Novembre 2021 - Mars 2022</span>\n                        <br></br>\n                        -\n                        <br></br>\n                        \"Dans ma poche, ma mémoire\", un podcast en 5 épisodes réalisé par Charlie\n                        Droesch-Du Cerceau durant les ateliers d'écriture menés à l'EHPAD du Centre\n                        Hospitalier d'Erstein. Projet initié par la Compagnie Actémo Théâtre en\n                        collaboration avec le Service Culturel de la Ville d'Erstein.\n                    </p>\n                </div>\n            </div>\n\n            <div className=\"podcast-page\">\n\n                {podcasts.map((podcast) => (\n                    <div key={podcast.url} className=\"podcast-item\">\n                        <h3 className='titlepodcastepi'>{podcast.title}</h3>\n                        <p>{podcast.texte}</p>\n                        <p>{podcast.date}</p>\n                        <div className='position-buttonpod'>\n                            <button\n                                className=\"round-button play-button\"\n                                onClick={() => togglePlayPause(podcast.url)}>\n                                {currentlyPlaying === podcast.url && !isPaused\n                                    ? (<img src=\"/images/pause.png\" alt=\"Pause\"/>)\n                                    : (<img src=\"/images/play.png\" alt=\"Écouter\"/>)}\n                            </button>\n                            <button\n                                className=\"round-button2 download-button\"\n                                onClick={() => downloadPodcast(podcast.url)}>\n                                <img src=\"/images/download.png\" alt=\"Télécharger\"/>\n                            </button>\n                        </div>\n                    </div>\n                ))}\n            </div>\n\n            <audio ref={audioRef}/>\n            <Footer/>\n        </div>\n    );\n}\n\nexport default Podcast;\n",{"ruleId":"194","severity":1,"message":"195","line":2,"column":16,"nodeType":"196","messageId":"197","endLine":2,"endColumn":22},{"ruleId":"194","severity":1,"message":"198","line":2,"column":34,"nodeType":"196","messageId":"197","endLine":2,"endColumn":43},{"ruleId":"194","severity":1,"message":"199","line":9,"column":33,"nodeType":"196","messageId":"197","endLine":9,"endColumn":43},{"ruleId":"194","severity":1,"message":"200","line":9,"column":45,"nodeType":"196","messageId":"197","endLine":9,"endColumn":53},{"ruleId":"194","severity":1,"message":"201","line":10,"column":8,"nodeType":"196","messageId":"197","endLine":10,"endColumn":12},{"ruleId":"202","severity":1,"message":"203","line":52,"column":25,"nodeType":"204","endLine":52,"endColumn":88},{"ruleId":"205","severity":1,"message":"206","line":89,"column":21,"nodeType":"204","endLine":89,"endColumn":78},{"ruleId":"207","severity":1,"message":"208","line":91,"column":25,"nodeType":"204","endLine":91,"endColumn":59},{"ruleId":"205","severity":1,"message":"206","line":94,"column":33,"nodeType":"204","endLine":94,"endColumn":66},{"ruleId":"205","severity":1,"message":"206","line":97,"column":33,"nodeType":"204","endLine":97,"endColumn":66},{"ruleId":"205","severity":1,"message":"206","line":100,"column":33,"nodeType":"204","endLine":100,"endColumn":67},{"ruleId":"205","severity":1,"message":"206","line":103,"column":33,"nodeType":"204","endLine":103,"endColumn":65},{"ruleId":"202","severity":1,"message":"203","line":114,"column":21,"nodeType":"204","endLine":121,"endColumn":38},{"ruleId":"205","severity":1,"message":"206","line":199,"column":17,"nodeType":"204","endLine":205,"endColumn":51},{"ruleId":"205","severity":1,"message":"206","line":220,"column":33,"nodeType":"204","endLine":220,"endColumn":89},{"ruleId":"205","severity":1,"message":"206","line":228,"column":33,"nodeType":"204","endLine":228,"endColumn":89},{"ruleId":"20